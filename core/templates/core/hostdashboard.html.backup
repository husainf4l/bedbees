    if (startBtn) {
        console.log('✅ Start listing button found, adding backup event listener');
        startBtn.addEventListener('click', function(e) {
            console.log('🎯 Button clicked via event listener');
            if (e.target.onclick) {
                console.log('✅ Onclick exists, executing...');
            } else {
                console.log('⚠️ No onclick, calling startListingProcess directly');
                startListingProcess();
            }
        });
        
        // Test button immediately
        console.log('🔍 Button test:', {
            id: startBtn.id,
            disabled: startBtn.disabled,
            onclick: !!startBtn.onclick,
            visible: !startBtn.classList.contains('hidden'),
            display: window.getComputedStyle(startBtn).display
        });
    } else {
        console.error('❌ Start listing button not found!');
    }
});

// Calendar functionality for Step 4
let currentDate = new Date();
let selectedDates = new Set();

function initCalendar() {
    updateCalendarDisplay();
    setupCalendarEventListeners();
}

function updateCalendarDisplay() {
    const monthYearElement = document.getElementById('calendar-month-year');
    const calendarGrid = document.getElementById('calendar-grid');
    
    if (!monthYearElement || !calendarGrid) return;
    
    const monthNames = [
        'January', 'February', 'March', 'April', 'May', 'June',
        'July', 'August', 'September', 'October', 'November', 'December'
    ];
    
    monthYearElement.textContent = `${monthNames[currentDate.getMonth()]} ${currentDate.getFullYear()}`;
    
    // Clear previous calendar
    calendarGrid.innerHTML = '';
    
    // Get first day of month and number of days
    const firstDay = new Date(currentDate.getFullYear(), currentDate.getMonth(), 1);
    const lastDay = new Date(currentDate.getFullYear(), currentDate.getMonth() + 1, 0);
    const daysInMonth = lastDay.getDate();
    const startingDayOfWeek = (firstDay.getDay() + 6) % 7; // Convert to Monday = 0
    
    // Add empty cells for previous month days
    for (let i = 0; i < startingDayOfWeek; i++) {
        const emptyDay = document.createElement('div');
        emptyDay.className = 'h-16 bg-gray-50 rounded border border-gray-200';
        calendarGrid.appendChild(emptyDay);
    }
    
    // Add days of current month
    for (let day = 1; day <= daysInMonth; day++) {
        const dayElement = document.createElement('div');
        dayElement.className = 'h-16 bg-white border border-gray-200 rounded cursor-pointer hover:bg-gray-50 transition-colors relative p-1';
        
        const dateKey = `${currentDate.getFullYear()}-${(currentDate.getMonth() + 1).toString().padStart(2, '0')}-${day.toString().padStart(2, '0')}`;
        
        // Day number
        const dayNumber = document.createElement('div');
        dayNumber.className = 'text-sm font-medium text-gray-900';
        dayNumber.textContent = day;
        dayElement.appendChild(dayNumber);
        
        // Price display (placeholder)
        const priceDisplay = document.createElement('div');
        priceDisplay.className = 'text-xs text-green-600 mt-1';
        priceDisplay.textContent = '$' + (100 + Math.floor(Math.random() * 50)); // Random price
        dayElement.appendChild(priceDisplay);
        
        // Status indicator
        const status = Math.random();
        let statusClass = 'bg-green-200 border-green-400'; // Available
        if (status < 0.1) {
            statusClass = 'bg-red-200 border-red-400'; // Blocked
        } else if (status < 0.2) {
            statusClass = 'bg-yellow-200 border-yellow-400'; // Booked
        } else if (status < 0.3) {
            statusClass = 'bg-blue-200 border-blue-400'; // Special rate
        }
        
        dayElement.className += ` ${statusClass}`;
        
        // Click handler
        dayElement.addEventListener('click', () => selectDate(dateKey, dayElement));
        
        calendarGrid.appendChild(dayElement);
    }
}

function setupCalendarEventListeners() {
    // Month navigation
    const prevButton = document.getElementById('prev-month');
    const nextButton = document.getElementById('next-month');
    
    if (prevButton) {
        prevButton.addEventListener('click', () => {
            currentDate.setMonth(currentDate.getMonth() - 1);
            updateCalendarDisplay();
        });
    }
    
    if (nextButton) {
        nextButton.addEventListener('click', () => {
            currentDate.setMonth(currentDate.getMonth() + 1);
            updateCalendarDisplay();
        });
    }
    
    // Bulk edit button
    const bulkEditBtn = document.getElementById('bulk-edit-btn');
    if (bulkEditBtn) {
        bulkEditBtn.addEventListener('click', showBulkEditModal);
    }
    
    // Quick action buttons
    const blockWeekendsBtn = document.getElementById('block-weekends');
    const setHolidayRatesBtn = document.getElementById('set-holiday-rates');
    const applySeasonalBtn = document.getElementById('apply-seasonal');
    
    if (blockWeekendsBtn) {
        blockWeekendsBtn.addEventListener('click', blockAllWeekends);
    }
    
    if (setHolidayRatesBtn) {
        setHolidayRatesBtn.addEventListener('click', setHolidayRates);
    }
    
    if (applySeasonalBtn) {
        applySeasonalBtn.addEventListener('click', applySeasonalRules);
    }
}

function selectDate(dateKey, element) {
    if (selectedDates.has(dateKey)) {
        selectedDates.delete(dateKey);
        element.classList.remove('ring-2', 'ring-green-500');
    } else {
        selectedDates.add(dateKey);
        element.classList.add('ring-2', 'ring-green-500');
    }
    
    console.log('Selected dates:', Array.from(selectedDates));
}

function showBulkEditModal() {
    if (selectedDates.size === 0) {
        alert('Please select dates first by clicking on calendar days.');
        return;
    }
    
    const action = prompt(`Bulk edit ${selectedDates.size} selected dates:\n\n1. Set price\n2. Block dates\n3. Unblock dates\n4. Set minimum stay\n\nEnter option number (1-4):`);
    
    switch (action) {
        case '1':
            const price = prompt('Enter new price:');
            if (price) {
                alert(`Price set to $${price} for ${selectedDates.size} dates`);
            }
            break;
        case '2':
            alert(`${selectedDates.size} dates blocked`);
            break;
        case '3':
            alert(`${selectedDates.size} dates unblocked`);
            break;
        case '4':
            const minStay = prompt('Enter minimum stay (nights):');
            if (minStay) {
                alert(`Minimum stay set to ${minStay} nights for ${selectedDates.size} dates`);
            }
            break;
    }
    
    // Clear selection
    selectedDates.clear();
    updateCalendarDisplay();
}

function blockAllWeekends() {
    if (confirm('Block all weekends (Friday-Sunday) for the current month?')) {
        alert('All weekends blocked for ' + currentDate.toLocaleDateString('en-US', { month: 'long', year: 'numeric' }));
        updateCalendarDisplay();
    }
}

function setHolidayRates() {
    const rate = prompt('Enter holiday rate multiplier (e.g., 1.5 for 50% increase):');
    if (rate) {
        alert(`Holiday rates set with ${rate}x multiplier`);
        updateCalendarDisplay();
    }
}

function applySeasonalRules() {
    alert('Seasonal pricing rules applied:\n\n• Summer months: +25%\n• Winter months: -15%\n• Holiday periods: +50%\n• Weekends: +20%');
    updateCalendarDisplay();
}

// Initialize calendar when step 4 is shown
document.addEventListener('DOMContentLoaded', function() {
    // Initialize calendar if on step 4
    setTimeout(() => {
        if (document.getElementById('calendar-grid')) {
            initCalendar();
        }
    }, 500);
});

// Override showStep function to initialize calendar for step 4
const originalShowStep = window.showStep;
if (originalShowStep) {
    window.showStep = function(stepNumber) {
        originalShowStep.call(this, stepNumber);

        // Initialize calendar for step 4
        if (stepNumber === 4) {
            setTimeout(() => {
                initCalendar();
            }, 100);
        }
    };
}

// Main Calendar Tab Functionality (Booking.com-style)
let mainCalendarDate = new Date();
let mainSelectedDates = new Set();
let calendarData = {}; // Stores pricing and availability data

function initMainCalendar() {
    updateMainCalendarDisplay();
}

function updateMainCalendarDisplay() {
    const monthYearElement = document.getElementById('main-calendar-month-year');
    const calendarGrid = document.getElementById('main-calendar-grid');

    if (!monthYearElement || !calendarGrid) return;

    const monthNames = [
        'January', 'February', 'March', 'April', 'May', 'June',
        'July', 'August', 'September', 'October', 'November', 'December'
    ];

    monthYearElement.textContent = `${monthNames[mainCalendarDate.getMonth()]} ${mainCalendarDate.getFullYear()}`;

    // Clear previous calendar
    calendarGrid.innerHTML = '';

    // Get calendar data
    const firstDay = new Date(mainCalendarDate.getFullYear(), mainCalendarDate.getMonth(), 1);
    const lastDay = new Date(mainCalendarDate.getFullYear(), mainCalendarDate.getMonth() + 1, 0);
    const daysInMonth = lastDay.getDate();
    const startingDayOfWeek = firstDay.getDay();

    const today = new Date();
    today.setHours(0, 0, 0, 0);

    // Add empty cells for days before month starts
    for (let i = 0; i < startingDayOfWeek; i++) {
        const emptyDay = document.createElement('div');
        emptyDay.className = 'h-24';
        calendarGrid.appendChild(emptyDay);
    }

    // Add days of current month
    for (let day = 1; day <= daysInMonth; day++) {
        const dayDate = new Date(mainCalendarDate.getFullYear(), mainCalendarDate.getMonth(), day);
        const dateKey = `${mainCalendarDate.getFullYear()}-${(mainCalendarDate.getMonth() + 1).toString().padStart(2, '0')}-${day.toString().padStart(2, '0')}`;

        const dayElement = document.createElement('div');
        const isPast = dayDate < today;
        const isSelected = mainSelectedDates.has(dateKey);

        // Get or generate day data
        if (!calendarData[dateKey]) {
            calendarData[dateKey] = {
                price: 100 + Math.floor(Math.random() * 100),
                status: isPast ? 'past' : (Math.random() > 0.8 ? 'booked' : 'available'),
                minStay: 1
            };
        }

        const dayData = calendarData[dateKey];

        // Base styling
        let bgClass = 'bg-gray-100 border-gray-200';
        if (isPast) {
            bgClass = 'bg-gray-50 border-gray-200 opacity-50 cursor-not-allowed';
        } else if (isSelected) {
            bgClass = 'bg-purple-100 border-purple-400 ring-2 ring-purple-500';
        } else if (dayData.status === 'available') {
            bgClass = 'bg-green-50 border-green-200 hover:bg-green-100';
        } else if (dayData.status === 'booked') {
            bgClass = 'bg-blue-50 border-blue-200';
        } else if (dayData.status === 'blocked') {
            bgClass = 'bg-red-50 border-red-200';
        }

        dayElement.className = `h-24 border-2 rounded-lg p-2 transition-all duration-200 ${bgClass} ${!isPast ? 'cursor-pointer' : ''}`;

        // Day number
        const dayNum = document.createElement('div');
        dayNum.className = 'font-semibold text-gray-900 text-sm mb-1';
        dayNum.textContent = day;
        dayElement.appendChild(dayNum);

        if (!isPast) {
            // Price
            const priceDiv = document.createElement('div');
            priceDiv.className = 'text-xs font-medium text-gray-700 mb-1';
            priceDiv.textContent = `$${dayData.price}`;
            dayElement.appendChild(priceDiv);

            // Status badge
            const statusDiv = document.createElement('div');
            statusDiv.className = 'text-xs';
            if (dayData.status === 'available') {
                statusDiv.className += ' text-green-600';
                statusDiv.textContent = '● Available';
            } else if (dayData.status === 'booked') {
                statusDiv.className += ' text-blue-600';
                statusDiv.textContent = '● Booked';
            } else if (dayData.status === 'blocked') {
                statusDiv.className += ' text-red-600';
                statusDiv.textContent = '● Blocked';
            }
            dayElement.appendChild(statusDiv);

            // Click handler
            dayElement.addEventListener('click', () => toggleDateSelection(dateKey));
        }

        calendarGrid.appendChild(dayElement);
    }

    // Update selected count
    const countElement = document.getElementById('selected-dates-count');
    if (countElement) {
        countElement.textContent = `${mainSelectedDates.size} dates selected`;
    }
}

function toggleDateSelection(dateKey) {
    if (mainSelectedDates.has(dateKey)) {
        mainSelectedDates.delete(dateKey);
    } else {
        mainSelectedDates.add(dateKey);
    }
    updateMainCalendarDisplay();
}

function changeMonth(delta) {
    mainCalendarDate.setMonth(mainCalendarDate.getMonth() + delta);
    updateMainCalendarDisplay();
}

function goToToday() {
    mainCalendarDate = new Date();
    updateMainCalendarDisplay();
}

function clearSelection() {
    mainSelectedDates.clear();
    updateMainCalendarDisplay();
}

function applyQuickPrice() {
    const price = document.getElementById('quick-price').value;
    if (!price || mainSelectedDates.size === 0) {
        alert('Please enter a price and select dates first.');
        return;
    }

    mainSelectedDates.forEach(dateKey => {
        if (calendarData[dateKey]) {
            calendarData[dateKey].price = parseInt(price);
        }
    });

    alert(`Price updated to $${price} for ${mainSelectedDates.size} selected dates`);
    mainSelectedDates.clear();
    updateMainCalendarDisplay();
}

function bulkEditMode() {
    if (mainSelectedDates.size === 0) {
        alert('Please select dates first by clicking on calendar days.');
        return;
    }

    const status = document.getElementById('availability-status').value;
    const minStay = document.getElementById('min-stay').value;

    mainSelectedDates.forEach(dateKey => {
        if (calendarData[dateKey]) {
            calendarData[dateKey].status = status;
            calendarData[dateKey].minStay = parseInt(minStay);
        }
    });

    alert(`Updated ${mainSelectedDates.size} dates:\n• Status: ${status}\n• Min Stay: ${minStay} nights`);
    mainSelectedDates.clear();
    updateMainCalendarDisplay();
}

function loadCalendarForListing(listingId) {
    if (!listingId) return;

    // In a real app, this would fetch data from the server
    alert(`Loading calendar for listing: ${listingId}\n\nIn production, this would:\n• Fetch pricing data from database\n• Load booking history\n• Sync with external calendars\n• Display real availability`);

    // Reset and reinitialize calendar
    calendarData = {};
    mainSelectedDates.clear();
    updateMainCalendarDisplay();
}

function importCalendar() {
    alert('📅 Import iCal Calendar\n\nFeatures:\n• Import from Airbnb, Booking.com, VRBO\n• Sync external calendars\n• Auto-block booked dates\n• Two-way sync support\n\nPaste iCal URL or upload .ics file');
}

function exportCalendar() {
    alert('📤 Export Calendar\n\nExport formats:\n• iCal (.ics)\n• CSV\n• Excel\n\nSync with:\n• Airbnb\n• Booking.com\n• Google Calendar\n• Outlook');
}

// Initialize main calendar when switching to calendar tab
const originalSwitchTab = window.switchTab;
if (originalSwitchTab) {
    window.switchTab = function(tabName) {
        originalSwitchTab.call(this, tabName);

        if (tabName === 'calendar') {
            setTimeout(() => {
                initMainCalendar();
            }, 100);
        }
    };
}

// Initialize on page load if calendar tab is active
document.addEventListener('DOMContentLoaded', function() {
    setTimeout(() => {
        if (document.getElementById('main-calendar-grid')) {
            initMainCalendar();
        }
    }, 500);
});
</script>

