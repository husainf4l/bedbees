function publishListing(type) {
    if (type === 'instant') {
        if (confirm('🚀 Publish your listing instantly?\n\nYour listing will go live immediately and start appearing in search results. Guests can book right away!\n\nAre you ready to start hosting?')) {
            // Simulate publishing process
            const button = event.target;
            button.innerHTML = '⏳ Publishing...';
