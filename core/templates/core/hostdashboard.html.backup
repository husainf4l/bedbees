<script>
function switchTab(tabName) {
    console.log('Switching to tab:', tabName);

    // Hide all content
    document.querySelectorAll('.tab-content').forEach(content => {
        content.classList.add('hidden');
    });

    // Remove active state from all nav items
    document.querySelectorAll('.nav-item').forEach(item => {
        item.classList.remove('text-green-600', 'bg-green-50');
        item.classList.add('text-gray-600');
    });

    // Show selected content
    const targetContent = document.getElementById('content-' + tabName);
    if (targetContent) {
        targetContent.classList.remove('hidden');
        console.log('Showing content for:', tabName);
    } else {
        console.error('Content section not found for:', tabName);
    }

    // Activate selected nav item
    const activeNav = document.getElementById('nav-' + tabName);
    if (activeNav) {
        activeNav.classList.remove('text-gray-600');
        activeNav.classList.add('text-green-600', 'bg-green-50');
    }

    // Update page title and subtitle
    const titles = {
        'overview': { title: 'Business Overview', subtitle: 'Manage your hospitality business' },
        'my-listings': { title: 'My Listings', subtitle: 'Manage your properties and tours' },
        'bookings': { title: 'Reservations', subtitle: 'View and manage bookings' },
        'accommodations': { title: 'Accommodations', subtitle: 'Manage your properties, amenities, and bookings' },
        'tours': { title: 'Tours & Experiences', subtitle: 'Manage your tour offerings and experiences' },
        'create-listing': { title: 'Create New Listing', subtitle: 'Step-by-step listing creation wizard' },
        'calendar': { title: 'Calendar & Pricing', subtitle: 'Manage availability and pricing' },
        'earnings': { title: 'Earnings & Payouts', subtitle: 'Track your income and manage payouts' },
        'guests': { title: 'Guest Management', subtitle: 'Manage your guests and communications' },
        'reviews': { title: 'Reviews & Ratings', subtitle: 'Guest feedback and ratings for your listings' },
        'verification': { title: 'Verification & Legal Documents', subtitle: 'Complete verification and compliance' },
        'profile': { title: 'My Profile', subtitle: 'Manage your hosting profile and preferences' }
    };

    if (titles[tabName]) {
        const pageTitle = document.getElementById('page-title');
        const pageSubtitle = document.getElementById('page-subtitle');
        if (pageTitle) pageTitle.textContent = titles[tabName].title;
        if (pageSubtitle) pageSubtitle.textContent = titles[tabName].subtitle;
    }
}

// Handle listing type selection to show relevant capacity fields
document.addEventListener('DOMContentLoaded', function() {
    // Initialize with overview tab (or use the active one)
    const activeNav = document.querySelector('.nav-item.active');
    if (activeNav) {
        const tabName = activeNav.id.replace('nav-', '');
        switchTab(tabName);
    } else {
        // Default to overview if no active tab
        switchTab('overview');
    }

    const listingTypeRadios = document.querySelectorAll('input[name="listing_type"]');
    const accommodationCapacity = document.getElementById('accommodation-capacity');
    const tourCapacity = document.getElementById('tour-capacity');
    const itinerarySection = document.getElementById('itinerary-section');
    const accommodationAmenities = document.getElementById('accommodation-amenities');
    const tourInclusions = document.getElementById('tour-inclusions');

    if (listingTypeRadios.length > 0) {
        listingTypeRadios.forEach(radio => {
            radio.addEventListener('change', function() {
                if (this.value === 'accommodation') {
                    // Show accommodation sections
                    if (accommodationCapacity) accommodationCapacity.classList.remove('hidden');
                    if (accommodationAmenities) accommodationAmenities.classList.remove('hidden');
                    // Hide tour sections
                    if (tourCapacity) tourCapacity.classList.add('hidden');
                    if (itinerarySection) itinerarySection.classList.add('hidden');
                    if (tourInclusions) tourInclusions.classList.add('hidden');
                } else if (this.value === 'tour') {
                    // Show tour sections
                    if (tourCapacity) tourCapacity.classList.remove('hidden');
                    if (itinerarySection) itinerarySection.classList.remove('hidden');
                    if (tourInclusions) tourInclusions.classList.remove('hidden');
                    // Hide accommodation sections
                    if (accommodationCapacity) accommodationCapacity.classList.add('hidden');
                    if (accommodationAmenities) accommodationAmenities.classList.add('hidden');
                } else {
                    // Hide all specific sections for package deals
                    if (accommodationCapacity) accommodationCapacity.classList.add('hidden');
                    if (tourCapacity) tourCapacity.classList.add('hidden');
                    if (itinerarySection) itinerarySection.classList.add('hidden');
                    if (accommodationAmenities) accommodationAmenities.classList.add('hidden');
                    if (tourInclusions) tourInclusions.classList.add('hidden');
                }
            });
        });
    }
});

